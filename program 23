#include <stdio.h>
#define INF 999
int main(){int n,src,i,j,u,v,min,d[20],vis[20],a[20][20];
    scanf("%d",&n);
    for(i=0;i<n;i++)for(j=0;j<n;j++)scanf("%d",&a[i][j]);
    scanf("%d",&src);
    for(i=0;i<n;i++){d[i]=a[src][i];vis[i]=0;}
    d[src]=0;vis[src]=1;
    for(i=1;i<n;i++){min=INF;
        for(j=0;j<n;j++)if(!vis[j]&&d[j]<min){min=d[j];u=j;}
        vis[u]=1;
        for(v=0;v<n;v++)if(!vis[v]&&d[u]+a[u][v]<d[v])d[v]=d[u]+a[u][v];
    }
    for(i=0;i<n;i++)printf("Dist(%dâ†’%d)=%d\n",src,i,d[i]);
}
