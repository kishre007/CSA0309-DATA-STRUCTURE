#include <stdio.h>
struct Node {
    int data;
    struct Node* left;
    struct Node* right;
};
void inorder(struct Node* root) {
    if (root != NULL) {
        inorder(root->left);
        printf("%d ", root->data);
        inorder(root->right);
    }
}
void preorder(struct Node* root) {
    if (root != NULL) {
        printf("%d ", root->data);
        preorder(root->left);
        preorder(root->right);
    }
}
void postorder(struct Node* root) {
    if (root != NULL) {
        postorder(root->left);
        postorder(root->right);
        printf("%d ", root->data);
    }
}
int main() {
    struct Node n1 = {1, NULL, NULL};
    struct Node n2 = {2, NULL, NULL};
    struct Node n3 = {3, NULL, NULL};
    struct Node n4 = {4, NULL, NULL};
    struct Node n5 = {5, NULL, NULL};
    n1.left = &n2;
    n1.right = &n3;
    n2.left = &n4;
    n2.right = &n5;
    printf("Inorder traversal: ");
    inorder(&n1);
    printf("\n");
    printf("Preorder traversal: ");
    preorder(&n1);
    printf("\n");
    printf("Postorder traversal: ");
    postorder(&n1);
    printf("\n");
}
