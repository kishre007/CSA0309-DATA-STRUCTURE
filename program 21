#include <stdio.h>
int q[20],front=-1,rear=-1,vis[20],a[20][20],n;
void bfs(int v){int i;
    for(i=0;i<n;i++) if(a[v][i] && !vis[i]) q[++rear]=i;
    if(front<=rear){vis[q[++front]]=1;printf("%d ",q[front]);bfs(q[front]);}
}
int main(){int i,j;
    scanf("%d",&n);
    for(i=0;i<n;i++)for(j=0;j<n;j++)scanf("%d",&a[i][j]);
    for(i=0;i<n;i++)vis[i]=0;
    vis[0]=1;printf("BFS: 0 ");bfs(0);
}
